{
  "name": "@laufire/kafka-router",
  "version": "0.1.0",
  "description": "Project description.",
  "main": "./dist/index.js",
  "scripts": {
    "dev": "node -e 'require(\"./dist/index.js\").main()'",
    "build": "rm -rf ./dist && babel --ignore '**/*.spec.js' -d ./dist ./src",
    "start": "npm run build && npm run dev",
    "test": "jest --no-cache",
    "lint": "eslint ./"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "engineStrict": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Laufire/kafka-router.git"
  },
  "keywords": [],
  "author": "Laufire Technologies",
  "license": "ISC",
  "homepage": "https://github.com/Laufire/kafka-router#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "acorn": "^6.3.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "eslint": "^5.16.0",
    "jest": "^24.9.0",
    "jest-cli": "^24.9.0",
    "node-rdkafka": "^2.7.1-2",
    "pino-pretty": "^3.2.2",
    "regenerator-runtime": "^0.13.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**/*.js"
    ],
    "coverageDirectory": "./.coverage",
    "coverageThreshold": {
      "global": {
        "branches": 0,
        "functions": 0,
        "lines": 0,
        "statements": 0
      }
    },
    "resetMocks": true,
    "resetModules": true,
    "setupFilesAfterEnv": [
      "./test/setup/jest.setup.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "dependencies": {
    "pino": "^5.13.4"
  },
  "bugs": {
    "url": "https://github.com/Laufire/kafka-router/issues"
  }
}
